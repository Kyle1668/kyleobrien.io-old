{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "history": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^2.2.7",
    "react-particles-js": "^2.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.1",
    "react-social-icons": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "format": "./node_modules/.bin/prettier --write '**/*.{js,jsx}'",
    "lint": "./node_modules/.bin/eslint '**/*.{js,jsx}' --quiet",
    "eject": "react-scripts eject",
    "prod-docker": "docker build -t kyleobrien.io . && docker run -p 3000:3000 --name portfilio_instance kyleobrien.io",
    "dev-docker": "docker build -t kyleobrien.io . && docker run -v ./:/portfolio/ -p 3000:3000 --name portfilio_instance kyleobrien.io"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.17.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
